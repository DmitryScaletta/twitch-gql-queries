{
  "name": "twitch-gql-queries",
  "version": "0.1.1",
  "description": "Type-safe requests to twitch public GraphQL API",
  "keywords": [
    "twitch",
    "graphql"
  ],
  "author": "DmitryScaletta",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DmitryScaletta/twitch-gql-queries.git"
  },
  "bugs": {
    "url": "https://github.com/DmitryScaletta/twitch-gql-queries/issues"
  },
  "types": "dist/index.d.ts",
  "module": "dist/index.js",
  "main": "dist/index.cjs",
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "scripts": {
    "build": "tsup",
    "gen": "node --no-warnings=ExperimentalWarning scripts/gen.ts",
    "new-query": "node --no-warnings=ExperimentalWarning scripts/new-query.ts",
    "test": "node --no-warnings=ExperimentalWarning --test",
    "test:watch": "node --no-warnings=ExperimentalWarning --test --watch",
    "test:types": "pnpm --package=typescript dlx tsc --noEmit --module nodenext --target esnext --allowImportingTsExtensions src/gqlRequest.test-d.ts"
  },
  "devDependencies": {
    "@sinclair/typebox": "^0.34.30",
    "@types/node": "^22.13.10",
    "json-schema-to-typescript": "^15.0.4",
    "prettier": "^3.5.3",
    "tsup": "^8.4.0",
    "typescript": "^5.8.2"
  },
  "packageManager": "pnpm@10.6.3+sha512.bb45e34d50a9a76e858a95837301bfb6bd6d35aea2c5d52094fa497a467c43f5c440103ce2511e9e0a2f89c3d6071baac3358fc68ac6fb75e2ceb3d2736065e6"
}
